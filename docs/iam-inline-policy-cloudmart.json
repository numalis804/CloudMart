{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "FullAccessAllServices",
			"Effect": "Allow",
			"Action": [
				"s3:*",
				"dynamodb:*",
				"ec2:*",
				"eks:*",
				"rds:*",
				"elasticache:*",
				"ecr:*",
				"cloudwatch:*",
				"logs:*",
				"cognito-idp:*",
				"cognito-identity:*",
				"lambda:*",
				"apigateway:*",
				"secretsmanager:*",
				"ssm:*",
				"kms:*",
				"elasticloadbalancing:*",
				"autoscaling:*",
				"cloudformation:*",
				"wafv2:*",
				"waf-regional:*",
				"guardduty:*",
				"cloudfront:*",
				"route53:*",
				"acm:*",
				"backup:*",
				"sns:*",
				"sqs:*",
				"events:*",
				"application-autoscaling:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "IAMPermissions",
			"Effect": "Allow",
			"Action": [
				"iam:Get*",
				"iam:List*",
				"iam:PassRole",
				"iam:CreateRole",
				"iam:DeleteRole",
				"iam:AttachRolePolicy",
				"iam:DetachRolePolicy",
				"iam:PutRolePolicy",
				"iam:DeleteRolePolicy",
				"iam:CreateServiceLinkedRole",
				"iam:CreatePolicy",
				"iam:DeletePolicy",
				"iam:CreatePolicyVersion",
				"iam:DeletePolicyVersion",
				"iam:CreateInstanceProfile",
				"iam:DeleteInstanceProfile",
				"iam:AddRoleToInstanceProfile",
				"iam:RemoveRoleFromInstanceProfile",
				"iam:TagRole",
				"iam:UntagRole",
				"iam:UpdateAssumeRolePolicy"
			],
			"Resource": "*"
		},
		{
			"Sid": "CloudTrailReadOnly",
			"Effect": "Allow",
			"Action": [
				"cloudtrail:DescribeTrails",
				"cloudtrail:GetTrailStatus",
				"cloudtrail:LookupEvents",
				"cloudtrail:ListTrails"
			],
			"Resource": "*"
		},
		{
			"Sid": "ServiceQuotasAndSupport",
			"Effect": "Allow",
			"Action": [
				"servicequotas:*",
				"support:*",
				"trustedadvisor:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "STSPermissions",
			"Effect": "Allow",
			"Action": [
				"sts:GetSessionToken",
				"sts:GetCallerIdentity",
				"sts:AssumeRole",
				"sts:DecodeAuthorizationMessage"
			],
			"Resource": "*"
		},
		{
			"Sid": "ChangeOwnPassword",
			"Effect": "Allow",
			"Action": [
				"iam:ChangePassword",
				"iam:GetUser",
				"iam:ListMFADevices"
			],
			"Resource": "arn:aws:iam::*:user/${aws:username}"
		},
		{
			"Sid": "TagResources",
			"Effect": "Allow",
			"Action": [
				"tag:GetResources",
				"tag:GetTagKeys",
				"tag:GetTagValues",
				"tag:TagResources",
				"tag:UntagResources"
			],
			"Resource": "*"
		},
		{
			"Sid": "CostAndBilling",
			"Effect": "Allow",
			"Action": [
				"ce:*",
				"cur:*",
				"budgets:*"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"budgets:ModifyBudget",
				"budgets:ViewBudget",
				"budgets:CreateBudgetAction",
				"budgets:DeleteBudgetAction",
				"budgets:DescribeBudgetAction",
				"budgets:DescribeBudgetActionHistories",
				"budgets:ExecuteBudgetAction",
				"budgets:UpdateBudgetAction"
			],
			"Resource": "*"
		}
	]
}